{
  "name": "mondanterist",
  "version": "1.0.0",
  "description": "a pinterest clone in react + redux",
  "main": "server/server.js",
  "engines": {
    "node": "5.7.0"
  },
  "scripts": {
    "test": "jest",
    "start": "npm run clean && webpack && nodemon server/server.js",
    "clean": "rm -rf dist",
    "deploy": "NODE_ENV=production && webpack"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ModanBrothers/mondanterist.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack"
  ],
  "author": "ModanBrothers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ModanBrothers/mondanterist/issues"
  },
  "homepage": "https://github.com/ModanBrothers/mondanterist#readme",
  "dependencies": {
    "autoprefixer": "^6.3.3",
    "babel-loader": "6.2.4",
    "bootstrap-loader": "^1.0.9",
    "bootstrap-sass": "^3.3.6",
    "css-loader": "^0.23.1",
    "dotenv": "2.0.0",
    "eslint-loader": "1.3.0",
    "file-loader": "^0.8.5",
    "history": "2.0.0",
    "html-webpack-plugin": "2.9.0",
    "immutable": "3.7.6",
    "imports-loader": "^0.6.5",
    "jquery": "^2.2.1",
    "koa": "1.1.2",
    "koa-connect-history-api-fallback": "0.3.0",
    "koa-static": "2.0.0",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.1",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-redux": "4.4.0",
    "react-router": "2.0.0",
    "react-router-redux": "4.0.0",
    "redux": "3.3.1",
    "redux-thunk": "1.0.3",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.1.2",
    "sass-resources-loader": "^1.0.2",
    "style-loader": "^0.13.0",
    "tether": "^1.2.0",
    "url": "0.11.0",
    "url-loader": "^0.5.7",
    "webpack": "1.12.14"
  },
  "devDependencies": {
    "babel-cli": "6.6.0",
    "babel-core": "6.6.0",
    "babel-eslint": "5.0.0",
    "babel-jest": "^9.0.1",
    "babel-plugin-react-transform": "2.0.0",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.0",
    "babel-preset-stage-0": "6.5.0",
    "babelify": "7.2.0",
    "enzyme": "^2.0.0",
    "eslint": "2.2.0",
    "eslint-config-airbnb": "6.0.2",
    "eslint-plugin-babel": "3.1.0",
    "eslint-plugin-react": "4.1.0",
    "jasmine": "^2.4.1",
    "jest-cli": "^0.9.0",
    "koa-webpack-dev-middleware": "1.1.0",
    "react-addons-test-utils": "^0.14.7",
    "redux-devtools": "3.1.1",
    "redux-devtools-dock-monitor": "1.1.0",
    "redux-devtools-log-monitor": "1.0.5",
    "webpack-hot-middleware": "2.9.0"
  }
